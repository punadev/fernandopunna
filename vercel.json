{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {"src": "/assets/(.*)", "headers": {"cache-control": "max-age=31536000,immutable"}, "dest": "/assets/$1"},
    {"src": "/(.*)", "headers": {"cache-control": "max-age=0,no-cache"}, "dest": "/index.html"}
  ]
}